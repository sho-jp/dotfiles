if is_os_mac
    # Alias Setting
    alias ls='ls -aG'
    alias ll='ls -al'

    # Alias GNU Comand
    alias sed='gsed'
    alias grep='ggrep'
    alias awk='gawk'
    alias find='gfind'
    alias xargs='gxargs'
    alias tar='gtar'

    # ls Color dir
    export LSCOLORS=gxfxcxdxbxegedabagacad
end
